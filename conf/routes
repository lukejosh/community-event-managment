# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.ApplicationController.index

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


GET /event/:id controllers.ApplicationController.getEvent(id:Integer)

GET /createevent controllers.ApplicationController.createEvent()

GET /saveevent controllers.ApplicationController.saveEvent()

GET /index controllers.ApplicationController.index()

GET /events controllers.ApplicationController.getRecentEvents()

POST /login controllers.ApplicationController.saveUser()



#Special TEST route for json output of model